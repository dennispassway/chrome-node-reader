{
  "manifest_version": 2,
  "name": "Read node_modules",
  "description": "Discovers what node_modules are used on that website",
  "version": "1.0",
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["content.js"]
  }],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
